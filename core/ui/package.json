{
  "name": "eisen-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/main.js --format=iife --minify && cp src/style.css dist/",
    "build:win": "esbuild src/main.ts --bundle --outfile=dist/main.js --format=iife --minify && node -e \"require('node:fs').copyFileSync('src/style.css','dist/style.css')\"",
    "dev": "esbuild src/main.ts --bundle --outfile=dist/main.js --format=iife --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "d3-force-3d": "^3.0.6",
    "force-graph": "^1.51.0"
  }
}
