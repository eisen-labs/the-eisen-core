{
  "name": "eisen-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "cp src/style.css ../extension/media/graph.css && esbuild src/main.ts --bundle --outfile=../extension/media/graph.js --format=iife --minify",
    "dev": "esbuild src/main.ts --bundle --outfile=../extension/media/graph.js --format=iife --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "d3-force-3d": "^3.0.6",
    "force-graph": "^1.51.0"
  }
}
